.online-index{
  display:flex;
  display:-webkit-flex;
  position: fixed;
  height: 100%;
  width: 100%;
  justify-content:center;
}
.menucategory{
  width:30vw;
  height:100%;
  padding-bottom:50px;
  background:#f8f8f8;
}
.menucategory view{
  padding:4vw 2vw;
  font-size:12px;
  color:#666;

}
.shopmenu{
  background:red;
}
.menucategory view span{
  text-overflow:ellipsis;
  overflow:hidden;
  display:-webkit-box;
}